{
	"name": "vue-autoimport",
	"displayName": "vue-autoimport",
	"description": "Provide of auto import feature for component.",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.47.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:vue-autoimport.helloWorld"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [{
			"command": "vue-autoimport.helloWorld",
			"title": "Hello World"
		}]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint '*/**/*.{js,ts}' --quiet --fix",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "jest"
	},
	"husky": {
		"hooks": {
			"pre-commit": "tsc --noEmit && lint-staged"
		}
	},
	"lint-staged": {
		"*.{js,ts}": [
			"eslint --fix",
			"git add"
		]
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/jest": "^26.0.9",
		"@types/node": "^13.11.0",
		"@types/vscode": "^1.47.0",
		"@typescript-eslint/eslint-plugin": "^2.34.0",
		"@typescript-eslint/parser": "^2.34.0",
		"eslint": "^6.8.0",
		"eslint-config-prettier": "^6.11.0",
		"eslint-plugin-prettier": "^3.1.4",
		"glob": "^7.1.6",
		"husky": "^4.2.5",
		"jest": "^26.2.2",
		"lint-staged": "^10.2.11",
		"prettier": "^2.0.5",
		"ts-jest": "^26.1.4",
		"typescript": "^3.9.7"
	}
}